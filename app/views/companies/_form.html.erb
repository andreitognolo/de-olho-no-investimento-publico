<%= form_for @company do |f| %>

    <div class="form-group">
      <%= f.label :name %>
      <%= f.text_field :name, size: 30, class: "form-control" %>
    </div>

    <div class="form-group">
      <%= f.label :cnpj %>
      <%= f.text_field :cnpj, class: "form-control" %>
    </div>

    <div class="form-group">
      <%= f.label :state %>
      <%= f.select :state, states %>
    </div>

    <%= f.submit "Salvar", class: "btn btn-default" %>
<% end %>